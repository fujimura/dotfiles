[core]
  pager = less
  editor = vim
	excludesfile = ~/.gitignore
[color]
  status = auto
  diff = auto
[alias]
  st = status -bs
  s = status -bs
  di = diff
  d  = diff
  ci = commit -v
  commit = commit -v
  co = checkout
  ad = add
  l = log
  lp = log --stat -p --graph
